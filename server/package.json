{
  "name": "leetnotion-backend",
  "version": "1.0.0",
  "description": "Backend for leetnotion website",
  "main": "./dist/app.ts",
  "scripts": {
    "start": "node ./dist/app.js",
    "build": "yarn clean && tsc && yarn copy-files",
    "watch": "nodemon --exec \"yarn build && yarn start\" --watch src --ext ts",
    "dev": "yarn build && ts-node-dev -r tsconfig-paths/register src/app.ts",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.graphql dist/"
  },
  "repository": "https://github.com/codewithsathya/leetnotion",
  "author": "codewithsathya <codewithsathya@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "-": "^0.0.1",
    "axios": "^0.27.2",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.6.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie": "^0.5.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.7.16",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^4.1.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.8.3"
  },
  "_moduleAliases": {
    "@app-root": "dist",
    "@app-lib": "dist/lib",
    "@app-models": "dist/models"
  }
}
